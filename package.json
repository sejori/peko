{
  "name": "peko",
  "version": "2.2.0",
  "description": "Featherweight apps on the edge, built with Web Standards üê£‚ö°",
  "main": "mod.ts",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "start": "deno run --allow-net --allow-read --allow-env scripts/deno.ts",
    "start:dev": "deno run --allow-net --allow-read --allow-env --watch scripts/deno.ts",
    "test": "deno test --allow-read --allow-net",
    "profile": "deno run --allow-read --allow-net tests/scripts/profile.ts",
    "test:bun": "bun run scripts/bun.ts",
    "test:wrangler": "wrangler dev scripts/cloudflareWorker.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sejori/peko.git"
