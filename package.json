  },
  "homepage": "https://github.com/sejori/peko#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240222.0",
    "wrangler": "^3.30.1"
  },
  "dependencies": {
    "htm": "^3.1.1",
    "preact": "^10.19.6",
    "preact-render-to-string": "^6.4.0"
  }
