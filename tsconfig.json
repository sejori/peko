{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "allowImportingTsExtensions": true,
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "lib/*": ["./lib/*"]
    }
  },
  "include": ["mod.ts"]
}